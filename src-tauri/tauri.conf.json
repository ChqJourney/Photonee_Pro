{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Image_Try",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "protocol": {
        "all": false,
        "asset": true,
        "assetScope": [
          "*"
        ]
      },
      "dialog": {
        "all": true,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": false
      },
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs":{
        "all": true,
        "copyFile": false,
        "createDir": false,
        "exists": false,
        "readDir": false,
        "readFile": true,
        "removeDir": false,
        "removeFile": false,
        "renameFile": false,
        "scope": [
          "**"
        ],
        "writeFile": false
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": "default-src: 'self'; img-src 'self' asset:"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Image_Try",
        "width": 800,
        "height": 600
      }
    ]
  }
}
